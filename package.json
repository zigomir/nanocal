{
  "name": "nanocal",
  "version": "0.4.0",
  "module": "dist/nanocal.min.js",
  "main": "dist/nanocal.min.js",
  "unpkg": "dist/nanocal.min.js",
  "repository": "https://github.com/zigomir/nanocal",
  "license": "MIT",
  "homepage": "https://github.com/zigomir/nanocal",
  "bugs": "https://github.com/zigomir/nanocal/issues",
  "description": "Minimal renderless date picker",
  "scripts": {
    "dev": "tsc -w & npm run dev:rollup & servor . index.html --no-browser --no-reload",
    "dev:rollup": "rollup src/main.js -c -w",
    "build": "rm -f dist/* && tsc && npm run build:rollup && bundlesize",
    "build:rollup": "rollup src/main.js -c",
    "prepublishOnly": "npm run build"
  },
  "bundlesize": [
    {
      "path": "dist/nanocal.min.js",
      "maxSize": "5 kB"
    }
  ],
  "dependencies": {
    "cntdys": "^0.2.8"
  },
  "devDependencies": {
    "bundlesize": "^0.18.0",
    "rollup": "^1.19.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "servor": "^2.2.1",
    "svelte": "^3.7.0",
    "typescript": "^3.5.3"
  }
}
